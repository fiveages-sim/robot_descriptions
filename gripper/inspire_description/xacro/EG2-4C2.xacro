      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find inspire_description)/meshes/EG2-4C2/Link1.obj"/>
        </geometry>
      </collision>
    </link>
    
    <joint name="${prefix}gripper_joint" type="revolute">
      <origin xyz="-0.04 -0.009 0.079"/>
      <parent link="${prefix}gripper_base"/>
      <child link="${prefix}Link1"/>
      <axis xyz="0 -1 0"/>
      <limit lower="0" upper="0.82" effort="1" velocity="1"/>
    </joint>
    
    <link name="${prefix}Link2">
      <inertial>
        <origin xyz="0.0087435 0.009 0.013955" rpy="0 0 0"/>
        <mass value="0.0038971"/>
        <inertia
            ixx="5.6941E-07"
            ixy="-1.0711E-18"
            ixz="-2.6891E-07"
            iyy="6.2305E-07"
            iyz="-1.6845E-18"
