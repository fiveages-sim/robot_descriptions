      </inertial>
      <visual>
        <geometry>
          <mesh filename="file://$(find inspire_description)/meshes/EG2-4C2/Link2.obj"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find inspire_description)/meshes/EG2-4C2/Link2.obj"/>
        </geometry>
      </collision>
    </link>
    
    <joint name="${prefix}gripper_joint2" type="revolute">
      <origin xyz="-0.03 -0.009 0.081" rpy="0 0 0"/>
      <parent link="${prefix}gripper_base"/>
      <child link="${prefix}Link2"/>
      <axis xyz="0 -1 0"/>
      <limit lower="0" upper="0.82" effort="1" velocity="1"/>
      <mimic joint="${prefix}gripper_joint"/>
